{
  "name": "petliz-api",
  "version": "1.0.0",
  "description": "Complete API built with Node.js and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "api": "json-server --watch db.json",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only 'src/index.ts' | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --runInBand --verbose",
    "test:unit": "jest",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axsilvasousa/petliz-api.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Ax de sousa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axsilvasousa/petliz-api/issues"
  },
  "homepage": "https://github.com/axsilvasousa/petliz-api#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "@types/helmet": "^4.0.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.17.2",
    "express-pino-logger": "^5.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.5",
    "nock": "^13.0.4",
    "pino": "6.3",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/multer": "^1.4.4",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.13.2",
    "@types/pino": "6.3",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "pino-pretty": "^4.2.1",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^3.8.3"
  }
}
